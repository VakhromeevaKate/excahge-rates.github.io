{
    "compilerOptions": {
      "outDir": "./dist/",
      "sourceMap": true,
      "noImplicitAny": true,
      "strict": true,
      "module": "es6",
      "target": "es5",
      "jsx": "react",
      "allowJs": true
    },
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "src/**/*.vue",
      "tests/**/*.ts",
      "tests/**/*.tsx",
      "src/**/*.html"
    ]
}
